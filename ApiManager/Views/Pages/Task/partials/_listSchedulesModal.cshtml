<!--begin::Modal Add schedule-->
<div class="modal fade" id="modal_list_schedules" data-bs-backdrop="static" tabindex="-1">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title">Schedules</h3>
                <!--begin::Close-->
                <div class="btn-active-icon-danger btn-sm btn-active-light-primary ms-2" data-kt-schedules-modal-action="close" aria-label="Close">
                    <!--begin::Svg Icon | path: icons/duotune/arrows/arr061.svg-->
                    <span class="svg-icon svg-icon-1">
                        <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="currentColor" />
                            <rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="currentColor" />
                        </svg>
                    </span>
                    <!--end::Svg Icon-->
                </div>
                <!--end::Close-->
            </div>

            <div class="modal-body">
                <input type="hidden" id="taskId" value="@Guid.Empty" />
                <!--begin::Toolbar-->
                <div class="d-flex justify-content-end" data-schedule-table-toolbar="base">
                    <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#modal_add_schedule">
                        <span class="svg-icon svg-icon-2">
                            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <rect opacity="0.5" x="11.364" y="20.364" width="16" height="2" rx="1" transform="rotate(-90 11.364 20.364)" fill="currentColor"></rect>
                                <rect x="4.36396" y="11.364" width="16" height="2" rx="1" fill="currentColor"></rect>
                            </svg>
                        </span>
                        Add Schedule
                    </button>
                </div>
                <!--begin::Table-->
                <table class="table align-middle table-row-dashed fs-6 gy-5" id="scheduleTable">
                    <!--begin::Table head-->
                    <thead>
                        <tr class="text-start text-muted fw-bold fs-7 text-uppercase gs-0">
                            <th class="w-10px pe-2">
                                <div class="form-check form-check-sm form-check-custom form-check-solid me-3">
                                    <input class="form-check-input" type="checkbox" data-kt-check="true" data-kt-check-target="#scheduleTable .form-check-input" value="1" />
                                </div>
                            </th>
                            <th>Schedule</th>
                            <th>Enabled</th>
                            <th class="text-end min-w-100px">Actions</th>
                        </tr>
                    </thead>
                    <!--end::Table head-->
                </table>
                <!--end::Table-->

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-light" data-kt-schedules-modal-action="cancel">Cancel</button>
            </div>
        </div>

    </div>
</div>
<!--end::Modal Add schedule-->
